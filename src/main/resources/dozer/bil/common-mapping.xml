			<b>enteDenominazione</b>
		</field>
	</mapping>
	
	<mapping map-id="SiacTEnteProprietario_Ente">
		<class-a>it.csi.siac.siaccorser.model.Ente</class-a>
		<class-b>it.csi.siac.siacbilser.integration.entity.SiacTEnteProprietario</class-b>		
